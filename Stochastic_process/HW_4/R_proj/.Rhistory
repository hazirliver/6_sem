N  <-  Tt/h
N
# Формирование одной траектории
pair.func <- function()
{
pair  <-  t(replicate(N,
sapply(c(1,2), function(y) rnorm(1, 0, sqrt(h)*sigma)),
simplify = T))
return(apply(rbind(c(0,0),pair), 2, cumsum))
}
